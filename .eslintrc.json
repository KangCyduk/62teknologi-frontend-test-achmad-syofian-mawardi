{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "react-hooks/exhaustive-deps": 0,
        "semi": 0,
        "quotes": 0,
        "react/prop-types": "off",
        "react/no-unknown-property": "off",
        "no-unused-vars":1,
        "no-case-declarations":0,
        "react/jsx-key": [0, { "checkFragmentShorthand": false,"checkKeyMustBeforeSpread":false }]
    }
}
